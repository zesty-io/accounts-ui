{
  "name": "accounts-settings",
  "version": "1.0.0",
  "description": "Manage your accounts settings",
  "main": "./src/index.js",
  "scripts": {
    "test": "../../../node_modules/.bin/jest",
    "start":
      "export NODE_ENV=\"development\" && ../../../node_modules/.bin/webpack --watch",
    "build-development":
      "export NODE_ENV=\"development\" && ../../../node_modules/.bin/webpack --progress",
    "build-stage":
      "export NODE_ENV=\"stage\" && ../../../node_modules/.bin/webpack --progress -p",
    "build-production":
      "export NODE_ENV=\"production\" && ../../../node_modules/.bin/webpack --progress -p"
  },
  "author": "Zesty.io<developers@zesty.io>",
  "license": "All Rights Reserved",
  "babel": {
    "presets": ["es2015", "react"]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
